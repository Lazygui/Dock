{
  "name": "Dock",
  "private": true,
  "type": "module",
  "version": "1.0.0",
  "main": "./electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "electron:dev": "concurrently \"pnpm run dev\" \"wait-on http://localhost:8080 && cross-env NODE_ENV=development  electron ./electron/main.js\"",
    "electron:build": "pnpm run build && electron-builder"
  },
  "build": {
    "appId": "com.example.dock",
    "productName": "Dock",
    "directories": {
      "output": "dock"
    },
    "files": [
      "./electron/main.js",
      "./dist/**/*",
      "electron/**/*"
    ],
    "asar": true,
    "win": {
      "target": "portable",
      "icon": "Dock.ico",
      "artifactName": "${productName}.exe"
    }
  },
  "dependencies": {
    "@codemirror/commands": "^6.10.1",
    "@codemirror/lang-markdown": "^6.5.0",
    "@codemirror/language": "^6.12.1",
    "@codemirror/language-data": "^6.5.2",
    "@codemirror/state": "^6.5.3",
    "@codemirror/theme-one-dark": "^6.1.3",
    "@codemirror/view": "^6.39.9",
    "@lezer/highlight": "^1.2.3",
    "@zumer/snapdom": "^2.0.1",
    "highlight.js": "^11.11.1",
    "iconv-lite": "^0.7.0",
    "jszip": "^3.10.1",
    "marked": "^17.0.1",
    "marked-highlight": "^2.2.3",
    "mermaid": "^11.12.2",
    "remixicon": "^4.7.0",
    "tcp-ping": "^0.1.1",
    "vue": "^3.5.21",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@types/codemirror": "^5.60.17",
    "@types/electron": "^1.6.12",
    "@types/marked": "^6.0.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "concurrently": "^9.2.1",
    "cross-env": "^10.0.0",
    "electron": "^38.1.2",
    "electron-builder": "^26.0.12",
    "sass": "^1.94.0",
    "sass-loader": "^16.0.6",
    "typescript": "~5.8.3",
    "vite": "npm:rolldown-vite@7.1.12",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6",
    "vue-tsc": "^3.0.7",
    "wait-on": "^9.0.1"
  },
  "pnpm": {
    "overrides": {
      "vite": "npm:rolldown-vite@7.1.12"
    },
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "electron",
      "electron-winstaller"
    ]
  }
}